@page 
@using Microsoft.Extensions.Localization
@using ProgecDo.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model ProgecDo.Web.Pages.Projects.CreateModalModel
@inject IStringLocalizer<ProgecDoResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Project" asp-page="/Projects/CreateModal">
    <abp-modal>
        <abp-modal-header style="background-image: url('img/modalHeader.jpg');" title="@L["NewProject"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content/>
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel | AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>